{
  "name": "liqsol",
  "version": "0.1.0",
  "type": "module",
  "description": "TypeScript Solana bot with structured logging and validation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "tsx src/index.ts",
    "healthcheck": "tsx src/commands/healthcheck.ts",
    "decode:reserve": "tsx src/commands/decodeReserve.ts",
    "decode:obligation": "tsx src/commands/decodeObligation.ts",
    "snapshot:obligations": "tsx src/commands/snapshotObligations.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "solana",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "0.29.0",
    "@kamino-finance/klend-sdk": "7.3.9",
    "@solana/web3.js": "^1.95.3",
    "dotenv": "^16.4.5",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
