{
  "name": "liqsol",
  "version": "0.1.0",
  "type": "module",
  "description": "TypeScript Solana bot with structured logging and validation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "tsx src/index.ts",
    "healthcheck": "tsx src/commands/healthcheck.ts",
    "decode:reserve": "tsx src/commands/decodeReserve.ts",
    "decode:obligation": "tsx src/commands/decodeObligation.ts",
    "oracle:debug": "tsx src/commands/oracleDebug.ts",
    "oracle:debug:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_oracle_debug_wsl.ps1",
    "snapshot:obligations": "tsx src/commands/snapshotObligations.ts",
    "snapshot:obligations:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_snapshot_wsl.ps1",
    "snapshot:scored": "tsx src/commands/snapshotScoredObligations.ts",
    "snapshot:scored:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_snapshot_scored_wsl.ps1",
    "snapshot:candidates": "tsx src/commands/snapshotCandidates.ts",
    "snapshot:candidates:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_snapshot_candidates_wsl.ps1",
    "live:indexer": "tsx src/commands/liveIndexer.ts",
    "live:indexer:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_live_indexer_wsl.ps1",
    "fetch:fixture": "tsx scripts/fetch_fixture.ts",
    "verify:reserves": "tsx src/cli/verifyReserves.ts",
    "test:pr8:candidates": "tsx scripts/validate_candidates.ts",
    "test:pr8:candidates:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_pr8_candidates_wsl.ps1",
    "flashloan:dryrun:kamino": "tsx src/commands/flashloanDryRunKamino.ts",
    "flashloan:dryrun:kamino:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_flashloan_dryrun_kamino_wsl.ps1",
    "test:pr9:flashloan:kamino": "tsx scripts/validate_pr9_flashloan_kamino.ts",
    "test:pr9:flashloan:kamino:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_pr9_flashloan_kamino_wsl.ps1",
    "test:flashloan:forecast": "tsx scripts/test_flashloan_dryrun_with_forecast.ts",
    "test:flashloan:forecast:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_flashloan_forecast_wsl.ps1",
    "prediction:test:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_prediction_pr85_wsl.ps1",
    "test:scheduler:forecast": "tsx scripts/test_scheduler_with_forecast.ts",
    "test:scheduler:forecast:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_scheduler_forecast_wsl.ps1",
    "audit:forecasts": "npx tsx src/commands/auditForecasts.ts",
    "audit:forecasts:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_audit_forecasts_wsl.ps1",
    "audit:pipeline": "npx tsx src/commands/auditPipeline.ts",
    "audit:pipeline:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_audit_pipeline_wsl.ps1",
    "test:forecast-ttl-refresh": "npx tsx scripts/test_forecast_ttl_refresh.ts",
    "test:forecast-ttl-refresh:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_forecast_ttl_refresh_wsl.ps1",
    "executor:dry": "tsx src/execute/executor.ts --dryrun",
    "executor:dry:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_executor_dry_wsl.ps1",
    "scheduler:main": "tsx src/scheduler/botStartupScheduler.ts",
    "scheduler:main:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_scheduler_main_wsl.ps1",
    "test:forecast-realtime-refresh": "tsx scripts/test_forecast_realtime_refresh.ts",
    "test:forecast-realtime-refresh:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_forecast_realtime_refresh_wsl.ps1",
    "test:yellowstone:smoke": "tsx scripts/test_yellowstone_smoke.ts",
    "test:yellowstone:smoke:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_yellowstone_smoke_wsl.ps1",
    "test:kamino:liquidation:build": "tsx scripts/test_kamino_liquidation_build.ts",
    "test:kamino:liquidation:build:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_kamino_liquidation_build_wsl.ps1",
    "test:jupiter:swapbuilder": "tsx scripts/test_jupiter_swapbuilder.ts",
    "test:jupiter:swapbuilder:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_jupiter_swapbuilder_wsl.ps1",
    "test:seized:delta": "tsx scripts/test_seized_delta_estimator.ts",
    "test:seized:delta:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_seized_delta_estimator_wsl.ps1",
    "test:executor:sim:full": "tsx scripts/test_executor_full_sim.ts",
    "test:executor:sim:full:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_executor_full_sim_wsl.ps1",
    "test:ttl:logic": "tsx scripts/test_ttl_expiry_logic.ts",
    "test:ttl:logic:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_ttl_expiry_logic_wsl.ps1",
    "test:ttl:rules": "tsx scripts/test_ttl_expiry_rules.ts",
    "test:ttl:rules:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_test_ttl_expiry_rules_wsl.ps1",
    "bot:run": "tsx src/bot/run.ts",
    "bot:run:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_bot_run_wsl.ps1",
    "pipeline:once": "npm run snapshot:obligations && npm run snapshot:scored && npm run snapshot:candidates && npm run test:scheduler:forecast && npm run audit:pipeline",
    "pipeline:once:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_pipeline_once_wsl.ps1",
    "check:rpc:guard": "npx tsx scripts/check_no_kamino_getRpc_web3_calls.ts",
    "check:rpc:guard:wsl": "powershell -ExecutionPolicy Bypass -File scripts/run_check_rpc_guard_wsl.ps1",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "solana",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "0.29.0",
    "@kamino-finance/klend-sdk": "7.3.9",
    "@kamino-finance/scope-sdk": "10.2.2",
    "@pythnetwork/client": "2.22.1",
    "@solana/web3.js": "^1.95.3",
    "@switchboard-xyz/switchboard-v2": "0.0.174",
    "@triton-one/yellowstone-grpc": "5.0.1",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.5",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
